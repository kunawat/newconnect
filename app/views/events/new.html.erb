<%#= link_to"Back to Calendar", {controller: "calendar", action: "show", :class => 'back-link'}, class: "btn btn-primary" %>

<div class="row" >
	 <div class="col-md-6 col-md-offset-3">
	<h2>Add Event</h2>
	 <div class="line"> </div>
	<%= form_for(:event , :url => {:action => 'create'}) do |f|%>
	<%= render(:partial => "form", :locals=> {:f=>f})%>
	
	<div class="form-buttons">
		<%= submit_tag("Create Event")%>
	</div>
	
	<% end %>
</div>	
</div>	