<br>
	<% whichpoll = Poll.find_by_id(session[:poll_id]) %>
		<% alloptions = whichpoll.poll_options %>
		<h1 style="margin-left:5px"> <%=whichpoll.question%> </h1>
		<br>
		<div>
			<table class="table table-hover table-condensed" style="margin-left:5px">
			    <thead>
			        <tr>
			            <th>Option</th>
			            <th>Number of Votes</th>
			        </tr>
		    	</thead>
				<% alloptions.each do |item| %>
					<tbody>
						<tr>
							<td>
								<%= item.keyword %>
							</td>
							<td>
					            <%= item.votes %>
					            <br> 
						 	 </td>
						</tr>
					</tbody>
				<% end %>
				<tfoot>
				 
				</tfoot>
			</table>
		</div>